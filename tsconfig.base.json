{
    "compilerOptions": {
        "alwaysStrict": true,
        "sourceMap": true,
        "declaration": true,
        "target": "es5",
        "removeComments": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "strictNullChecks": true,
        "stripInternal": true,
        "noFallthroughCasesInSwitch": true,
        "experimentalDecorators": true,
        "noEmitOnError": true,
        "downlevelIteration": true,
    },
}
