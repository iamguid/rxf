{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES5",
        "types": ["node"],
        "lib": ["ESNext", "ES6"],
        "alwaysStrict": true,
        "sourceMap": true,
        "declaration": true,
        "removeComments": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "strictNullChecks": true,
        "stripInternal": true,
        "noFallthroughCasesInSwitch": true,
        "experimentalDecorators": true,
        "noEmitOnError": true,
        "downlevelIteration": true,
    }
}
